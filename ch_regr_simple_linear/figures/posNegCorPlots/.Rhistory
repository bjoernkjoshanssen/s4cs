	buildAxis(1,x,n,nMax=nMax)#
	mtext(xlab, 1, 1.8)#
	mtext(ylab, 2, 2.8)#
	par(mgp=c(2.8,0.75,0), las=1)#
	buildAxis(2,y,n,nMax=nMax)#
	g <- lm(y ~ x)#
	abline(g, col='#00000088')#
}#
