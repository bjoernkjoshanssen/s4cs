plotWLine <- function(x,y,xlab='', ylab='',col='#225588AA',cex=1.7,pch=20,n=4,nMax=5,...){#
	par(las=0)#
	plot(x,y, axes=FALSE, pch=20, cex=1.7, col='#225588AA', ...)#
	buildAxis(1,x,n,nMax=nMax)#
	mtext(xlab, 1, 1.8)#
	mtext(ylab, 2, 2.8)#
	par(mgp=c(2.8,0.75,0), las=1)#
	buildAxis(2,y,n,nMax=nMax)#
	g <- lm(y ~ x)#
	abline(g, col='#00000088')#
}#
#
#
